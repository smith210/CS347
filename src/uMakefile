install: umake
	cp -p umake ${HOME}/bin/umake

who:
	echo The user is ${USER}asdf  ${USER. ${USER} . ${USER}. 

who_fail:
	echo ${USER   }

who1:
	echo The user is going to print 5 $\5 we printed 5 ${USER}. 

