umake: umake.o arg_parse.o target.o
	gcc -o umake umake.o arg_parse.o target.o
	cp -p umake bin
	export PATH=bin:${PATH}

umake.o: umake.c arg_parse.h target.h
	gcc -c umake.c 
	echo umake.o

arg_parse.o: arg_parse.c arg_parse.h
	gcc -c arg_parse.c
	echo arg_parse.o

target.o: target.c target.h
	gcc -c target.c
	echo target.o

